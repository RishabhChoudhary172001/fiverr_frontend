# .gitignore

# Ignore Node.js modules folder
node_modules/

# Ignore build artifacts
dist/

# Ignore environment variables file
.env

# Ignore package lock file
package-lock.json

# Ignore editor-specific files
.vscode/
*.sublime-project
*.sublime-workspace

# Ignore logs and other generated files
logs/
*.log
npm-debug.log*

# Ignore user-specific files
.DS_Store
Thumbs.db

# Ignore Vite config file
vite.config.js